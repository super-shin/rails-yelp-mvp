<h1>New Restaurant</h1>
<%= simple_form_for @restaurant do |f| %>
  <div class="form-group">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :category %>
  </div>
  <%= f.button :submit, "Create Restaurant", class: "btn btn-primary" %>
<% end %>
<%= link_to "Back to Restaurants List", restaurants_path %>
